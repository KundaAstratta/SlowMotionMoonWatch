<settings>

    <setting propertyKey="@Properties.HandColor" title="@Strings.HandColorTitle">
        <settingConfig type="list">
            <listEntry value="0xFFAA00">@Strings.ColorYellow</listEntry>
            <listEntry value="0x0000FF">@Strings.ColorBlue</listEntry>
            <listEntry value="0xFF0000">@Strings.ColorRed</listEntry>
            <listEntry value="0xFFFFFF">@Strings.ColorWhite</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.isShowNumbers" title="@Strings.isShowNumbersTitle">
        <settingConfig type="boolean" />
    </setting>

</settings>
